        <li class="tree-results compact">
          <b>
            <%= link_to(nos[:full_name],
                        plants_taxonomy_show_path(nos[:id]),
                        title: "Select to show Name details")
                      %>
          </b>
            <% if nos.show_status? %>
              <span class="name-status" title="Name Status"><%= nos.status.name %></span>
            <% end %>
              <span class="small">
                <%= "sensu #{nos.reference.citation_html}".html_safe %>
              </span>
              <span class="debug purple">[<strong><%#= nos.rank.name %></strong>]</span>

              <%= render partial: "/plants/taxonomy/accepted/search/citation_links", locals: {name: nos} %>

              <% if session[:taxonomy_details] %>
                  <%= render partial: "/plants/taxonomy/accepted/search/accepted_name_details", locals: {name: nos, visible: true} %>
              <% else %>
                  <%= render partial: "/plants/taxonomy/accepted/search/accepted_name_details", locals: {name: nos, visible: false} %>
              <% end %>
        </li>
