
 <li class="tree-results compact">
            <span class="debug purple">[<%= synonym.sort_name %>]</span>
            <span class="synonym">
              <%= link_to("#{synonym.full_name_html}".html_safe,
                        plants_taxonomy_show_path(synonym.id),
                        title: "Show bibliographic details")
                        %><span class="name-status"><%= synonym.status.name_to_show %></span>
                      <% if synonym.synonym_type.misapplied %> <span class="instance-type">auct. non</span> <%= synonym.synonym_name.author.try('abbrev') %>:
                <%= synonym.synonym_cites.this_cites.reference.author.name %>
                (<%= synonym.synonym_cites.this_cites.reference.year %>)
                [fide
                <%= synonym.synonym_ref.author.name %>
                (<%= synonym.synonym_ref.year %>)]
              <% end %>
            </span>
            <%= render partial: "/plants/taxonomy/accepted/search/citation_links", locals: {name: synonym} %>
            <%= '&nbsp;<span class="pro-parte">p.p.</span>'.html_safe if synonym.synonym_type.pro_parte? %>
            =
            <strong>
            <%= link_to("#{synonym.accepted_full_name}",
                        plants_taxonomy_show_path(synonym.accepted_id),
                        title: "Show bibliographic details")
                      %>
            </strong>
            <% if false && cbn.show_status? %>
              <span class="name-status" title="Name Status"><%#= cbn.status.name %></span>
            <% end %>
 </li>
