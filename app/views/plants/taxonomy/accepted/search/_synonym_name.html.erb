
 <li class="compact">
            <span class="debug purple">[<%= synonym.sort_name %>]</span>
            <em>
            <span class="synonym">
              <%= link_to("#{synonym.full_name}",
                        plants_taxonomy_accepted_show_path(synonym.id),
                        title: "Show bibliographic details")
                      %>
            </span>
            </em> 
            <span class="debug purple">[<%#= name.rank.name %>]</span>
            <%= render partial: "/plants/taxonomy/accepted/search/citation_links", locals: {name: synonym} %>
            =
            <strong>
            <%= link_to("#{synonym.accepted_full_name}",
                        plants_taxonomy_accepted_show_path(synonym.accepted_id),
                        title: "Show bibliographic details")
                      %>
            </strong>
            <% if false && cbn.show_status? %>
              <span class="name-status" title="Name Status"><%#= cbn.status.name %></span>
            <% end %>
 </li>

