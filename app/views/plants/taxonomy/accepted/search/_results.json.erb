



<% if @search.results.size > 0 %>
      <% @search.results.each_with_index do |name, ndx| %>
          <% if @search.parsed.show_details? &&
                ndx < session[:default_details_limit] %>
              <%= render partial: '/plants/names/search/details',
                     locals: {name: name} %>
              <br/>
          <% else %>
            <%= name.full_name %>
            <%= name.status.name %>
        <% end %>
      <% end %>
  <% end %>

