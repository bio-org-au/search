
<% if @search.results.size > 0 %>
    <ul class="taxonomy-results">
      <% @search.results.each_with_index do |name, ndx| %>
      <% if name.accepted_accepted? %>
        <%= render partial: "/plants/taxonomy/accepted/search/accepted_accepted_name", locals: {nos: name} %>
      <% elsif name.accepted_excluded? %>
        <%= render partial: "/plants/taxonomy/accepted/search/accepted_excluded_name", locals: {nos: name} %>
      <% else %>
        <li>Unknown or unexpected type of record: <%= name.type_code %></li>
      <% end %>
      <% end %>
    </ul>
  <% end %>


