<% content_for :page_title do %>
  Vascular Plant Taxonomy &mdash; Accepted
<% end %>
<% content_for :page_banner do %>
  <h1>Other taxonomies</h1>
<% end %>
<% content_for :sidebar do %>
  <%= render "sidebar" %>
<% end %>

<p>Biodiversity Australia supports taxonomies other than the nationally
accepted taxonomy.
</p>

<p>Those other taxonomies [will] include:
</p>
<ul>
<% Plants::Taxonomy::Other::TAXONOMIES.each_pair do | key, taxonomy | %>
  <li><%= link_to(taxonomy[:name], plants_taxonomy_other_show_path(key)) %></li> 
<% end %>
</ul>
<br>


<span class="debug hidden-print tiny">
  app/views/plants/taxonomy/other/index
</span>
